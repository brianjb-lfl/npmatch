{
  "name": "client",
  "authors": "Brian Besaw, Ben Malin, Brad Garner",
  "version": "0.1.0",
  "description": "npmatch",
  "homepage": "https://arcane-journey-50164.herokuapp.com/client",
  "engines": {
    "node": "8.2.1"
  },
  "license": "MIT",
  "main": "/server/server.js",
  "private": true,
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai-spies": "^0.7.1",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "deep-assign": "^2.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "knex": "^0.13.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "pg": "^7.3.0",
    "should": "^13.1.3",
    "tap": "^11.0.0",
    "tape": "^4.8.0",
    "test-console": "^1.1.0"
  },
  "scripts": {
    "start": "concurrently \"nodemon ./server/server.js\" \"cd client && react-scripts start\"",
    "test": "mocha --colors server/test/*test.js",
    "build": "concurrently \"cd client && react-scripts build\" \" --ignore node_modules,build --out-dir server/build\"",
    "coverage": "nyc mocha --exit --colors  server/test/*test.js",
    "nyc": "test",
    "cover": "nodemon --exec nyc --reporter=lcov --reporter=text-summary mocha --exit server/test/*test.js",
    "heroku": "node server/server.js",
    "heroku-postbuild": "cd ../client && npm install --only=dev && npm install && npm run build",
    "travis": "mocha --exit server/test/*test.js"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1"
  }
}
