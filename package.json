{
  "name": "client",
  "authors": "Brian Besaw, Ben Malin, Brad Garner",
  "version": "0.1.0",
  "description": "npmatch",
  "homepage": "https://arcane-journey-50164.herokuapp.com/client",
  "engines": {
    "node": "8.2.1"
  },
  "license": "MIT",
  "main": "/server/server.js",
  "private": true,
  "dependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "moment": "^2.19.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-widgets": "^4.1.1",
    "react-widgets-moment": "^4.0.4",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3",
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai-spies": "^0.7.1",
    "cors": "^2.8.4",
    "deep-assign": "^2.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "knex": "^0.13.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "pg": "^7.3.0",
    "should": "^13.1.3",
    "tap": "^11.0.0",
    "tape": "^4.8.0",
    "test-console": "^1.1.0"
  },
  "scripts": {
    "start": "nodemon ./server/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "cd client && npm start",
    "dev:server": "nodemon ./server/server.js",
    "build": "run-p build:client build:server",
    "build:client": "cd client && npm install --only=dev && npm install && npm run build",
    "build:server": "--ignore node_modules,build --out-dir server/build",
    "heroku-postbuild": "cd client && npm install --only=dev && npm run build",
    "test": "mocha --colors server/test/*.test.js",
    "coverage": "nyc mocha --colors  server/test/*test.js",
    "nyc": "test",
    "cover": "nodemon --exec nyc --reporter=lcov --reporter=text-summary mocha test/setup.test.js */*.test.js",
    "heroku": "node index.js",
    "travis": "mocha server/test/*test.js"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1"
  }
}
